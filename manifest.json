{
  "manifest_version": 3,
  "name": "GitHub AI PR",
  "version": "1.0.0",
  "description": "An extension for GitHub that uses AI to write PRs for you.",
  "permissions": [
    "contextMenus",
    "storage",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://github.com/*"
      ],
      "exclude_matches": [
        "https://*/login/*"
      ],
      "js": [
        "build/content.bundle.ts"
      ]
    }
  ]
}
